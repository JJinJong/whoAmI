/* 
필터 콘텐츠 
*/

.filter_content_wrapper{
	display:none;
	width: 100%;
    height: 100%;
	transform: translateZ(0px);
    position: fixed;
    left: 0px;
    top: 0px;
    padding: 40px;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0, 0, 0, 0.4);
    z-index: 5000;
    overflow: hidden;
}

.filter_content::-webkit-scrollbar {
	 display: none;
}

.filter_content{
position: fixed;
    top: 107px !important;
    inset: 40px 0px;
    width: 680px;
    height: 437px;
    overflow-y: scroll;
    justify-content: center;
    z-index: 100;
    margin: 0px auto;
    box-shadow: rgb(0 0 0 / 25%) 0px 15px 30px 0px;
    background-color: rgb(255, 255, 255);
    overflow: hidden;
}


.filter_content_header{
	display: flex;
    position: fixed;
    width: 680px;
    height: 7%;
    z-index: 1000;
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
    justify-content: center;
    align-items: center;
    box-shadow: rgb(240 240 240) 0px -1px 0px 0px inset;
    background-color: rgb(255, 255, 255);
}

.filter_content_header_cancel{
	position: absolute;
    top: 16px;
    bottom: 16px;
    cursor: pointer;
    left: 24px;
    height: 24px;
    font-size: 16px;
    letter-spacing: -0.7px;
    text-align: center;
    color: rgb(60, 152, 146);
}

.filter_content_header_title{
	width: 40px;
    font-size: 16px;
    font-weight: bold;
    letter-spacing: -0.7px;
    color: rgb(51, 51, 51);
}

.filter_content_section{
	margin-top: 81px;
    margin-bottom: 55px;
}

.filter_content_section_title{
	margin: 24px 0px 12px 24px;
    height: 25px;
    font-size: 17px;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: -0.7px;
    color: rgb(51, 51, 51);
    font-weight: bold;
}

.filter_content_section_text_wrapper{
	display: flex;
    justify-content: flex-start;
    padding-bottom: 24px;
    margin-left: 24px;
    margin-right: 24px;
    box-shadow: rgb(240 240 240) 0px -1px 0px 0px inset;
    flex-wrap: wrap;
}

/* 체크박스 수정 */
.filter_content_section_text input[type="checkbox"] {
	display: none;
}

.filter_content_section_text input[type="checkbox"] + label {
	display: inline-block; 
	background: url("data:image/png;base64,");
	cursor: pointer; 
	float: right;
    width: 24px;
    height: 24px;
    background-size: cover;
}

.filter_content_section_text input[type="checkbox"]:checked + label {
	background:url("data:image/png;base64,"); 
	background-size: cover;
	float: right
}



.filter_content_section_text {
	position:relative;
}

.filter_content_section_text input[type="checkbox"] + label span {
	position: absolute; 
	top: 0; 
	left: 23px;
	display: block; 
}

.filter_content_section_text:after {
	display:block; 
	clear:both; 
	content:"";
}

.filter_content_section_text{
	align-items: flex-start;
    margin-bottom: 12px;
    margin-top: 12px;
    position: relative;
    display: flex;
    flex-basis: 50%;
    flex-wrap: wrap;
}

.filter_content_section_text span{
	display: flex;
    justify-content: center;
    flex-direction: column;
    height: 24px;
    margin-left: 12px;
    font-size: 15px;
    letter-spacing: -0.7px;
    color: rgb(51, 51, 51);
    cursor: pointer;
}

.filter_content_section_img{
	display: flex;
    width: 24px;
    height: 24px;
    object-fit: contain;
}

/* 필터 푸터 */
.filter_content_footer_wrapper{
	display: flex;
    width: 780px;
    height: 56px;
    z-index: 1000;
    align-items: center;
    box-shadow: rgb(240 240 240) 0px 1px 0px 0px inset;
    background-color: rgb(255, 255, 255);
}

.filter_content_footer_reset{
	margin-left: 23px;
    cursor: pointer;
    height: 24px;
    font-size: 16px;
    letter-spacing: -0.7px;
    text-align: center;
    color: #4b4d91;
}

.filter_content_footer_button{
	position: absolute;
    right: 24px;
    width: 136px;
    height: 40px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #4b4d91;
