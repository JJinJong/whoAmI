@charset "UTF-8";


* {
    box-sizing: border-box;
    font-family: 'Spoqa Han Sans', sans-serif;
    letter-spacing: -0.7px;
}  

div {
    display: block;
}  


.writeWrapper{
    margin-top: 120px;
    margin-bottom: 98px;
}

        .page {
            max-width: calc(1164px + 32px);
            margin: 50px auto 0;
            margin-top: 20px;
            padding: 20px 16px;
        }

        .backTitle {
            max-width: 1164px;
            margin-bottom: 20px;
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            font-size: 1.3rem;
            font-weight: 300;
        }

        .back {
            position: relative;
            cursor: pointer;
            background-color: #4b4d91;
            background-color: #4b4d91;
            color: white;
            text-align: center;
            font-weight: 900;
            width: 35px;
            height: 35px;
            border-radius: 50%;
            font-size: 1.45rem;
            box-shadow: rgb(0 0 0 / 30%) 0px 2px 3px 0px;
            margin-right: 10px;
        }

        .topTitle {
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            font-size: 20px;
        }

        
        .writeCard {
            box-shadow: 0px 3px 6px 0 rgb(0 0 0 / 20%);
            border-radius: 5px;
            padding: 20px;
            background-color: #fff;
            border-top: 10px solid #4b4d91;
            margin-bottom: 50px;
            padding-bottom: 40px;
        }
        
        .header {
            display: flex;
            -webkit-box-align: center;
            flex-direction: row;
            align-items: center;
            margin: 10px 3%;
            justify-content: space-between;
        }

        .TimerWrapper {
            display: flex;
            -webkit-box-pack: center;
            color:gray; 
            font-size: 1.5rem;
            text-align: start;
            padding-top: 10px;
        }
        
        .options {
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            flex-wrap: wrap;
        }
        
        .OnOffWrapper {
            display: flex;
            -webkit-box-align: center;
            align-items: center;
        }
        
        .on-off.off {
            background-color: rgb(255, 255, 255);
        }
        
        .on-off {
            flex-shrink: 0;
            cursor: pointer;
            position: relative;
            width: 40px;
            height: 20px;
            border-radius: 15px;
            box-shadow: rgb(0 0 0 / 40%) 1px 2px 3px 0px inset;
            background-color: rgb(246, 239, 207);
            opacity: 0.95;
            transition: background-color 0.2s ease 0s;
        }
        
        .on-off.off .switch {
            left: 0px;
        }
        
        .on-off .switch {
            background-color: white;
            position: relative;
            top: 50%;
            left: 18px;
            width: 22px;
            height: 22px;
            transform: translateY(-50%);
            box-shadow: rgb(0 0 0 / 50%) 0px 2px 4px 0px;
            border-radius: 50%;
            transition: left 0.2s ease 0s;
        }
        
        .label.off {
            color: rgb(170, 170, 170);
        }
        
        .label {
            margin-left: 5px;
            font-size: 0.9rem;
            color: rgb(68, 68, 68);
            transition: color 0.2s ease 0s;
        }
        
        .option {
            margin-left: 10px;
        }
        
        .main {
            /* position: relative; */
            margin-top: 20px;
            /* resize: inline; */
            width: 100%;
            padding: 10px 10px 25px;
            border: none;
            box-shadow: rgb(0 0 0 / 20%) 0px 1px 4px 0px inset;
            outline: none;
            font-size: 0.9rem;
            border-radius: 15px;
            color: rgb(119, 119, 119);
            line-height: 1.5rem;
            margin-bottom: 100px;
        }

        .cardsWrapper {
            position: relative;
            top: 0px;
            left: 25px;
            width: 95%;
            text-align: center;
            /* padding-bottom: 20px; */
        }

        .cards {
            display: flex;
            flex-wrap: wrap;
            -webkit-box-pack: center;
            /* justify-content: center; */
            width: 100%;
            position: relative;
        }

        .detailWrapper {
            /* position: relative;
            transition: opacity 0.5s ease 0s, top;
            width: 14.28;
            height: 25%;
            padding-bottom: 1%;
            overflow: hidden;
            display: flex;
            flex-wrap: wrap;
            flex-direction: column;
            margin-bottom: 16px; */
            position: relative;
            transition: opacity 0.5s ease 0s, top;
            width: 12.5%;
            height: 30%;
            /* padding-left: 5%; */
            /* padding-bottom: 10.25%; */
            overflow: hidden;
            margin-bottom: 3%;
            opacity: 87%;
        }

        .cardWrapper {
            position: relative;
            transition: opacity 0.5s ease 0s, top;
            width: 100%;
            height: 50%;
            padding-bottom: 100%;
            overflow: hidden;
            display: flex;
            flex-wrap: wrap;
            /* margin-bottom: 5px; */
        }
        
        .card {
            position: relative;
            top: 0px;
            left: 0px;
            width: 90%;
            height: 90%;
            border-radius: 40%;
            padding: 50%;
            cursor: pointer;
        }

        .emotion {
            /* position: absolute;
            width: 100%;
            height: 50%;
            font-size: 6px;
            padding-bottom: 3%;
            top: 84%;
            left: -5px;
            text-align: center;
            cursor: pointer; */
            position: relative;
            transition: opacity 0.5s ease 0s, top;
            width: 90%;
            height: 40%;
            margin-top: -90%;
            margin-bottom: 3%;
            margin-left: 2%;
            box-shadow: rgb(0 0 0 / 15%) 0px 1px 2px 0px;
            border-radius: 7px;
            padding: 5% 0;
            /* padding-bottom: 10.25%; */
            /* overflow: hidden; */
        }

