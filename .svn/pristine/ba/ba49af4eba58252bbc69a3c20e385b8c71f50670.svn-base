        .step {
            /* box-shadow: rgb(0 0 0 / 20%) 0px 1px 4px 0px inset; */
            border-radius: 20px;
            margin-top: 30px;
            margin-bottom: 50px;
            padding-top: 10px;
            padding-left: 3%;
            width: 100%;
        }

        .q {
            font-size: medium;
            font-weight: 600;
            display: block;
            margin-left: 10px;
            margin-top: 10px;
            margin-bottom: 20px;
        }

        .a textarea {
            width: 95%;
            resize: none;
            height: 60px;
            border-radius: 10px;
            margin-top: 10px;
            padding: 20px;
            padding-top: 10px;
            border-color: rgb(207, 205, 205);
            appearance: auto;
            outline: none;
            border: 1px solid gainsboro;
        }

        .comfort {
            display: flex;
            justify-content: center;
            align-items: center;
            padding-bottom: 10px;
        }

        div.a span {
            display: block;
            margin-top: 10px;
            margin-left: 10px;
            font-weight: 300;
        }

        .writeCard .TimerWrapper {
            margin-left: 3%;
        }

        .comfort span {
            display: flex;
            height: fit-content;
            position: relative;

        }

        .comfort img {
            display: block;
            width: auto;
            height: 200px;
            opacity: 0.65;
            border-radius: 30px;
        }


        .pageFooter {
            position: relative;
            align-items: center;
    		justify-content: flex-end;
        }
        
        
        div.myPost {
            display: flex;
            width: 100%;
            height: 80px;
            align-items: center;
        }
        
        .pageFooter button {
            background-color: rgb(255, 255, 255);
            cursor: pointer;
            width: 20%;
            display: block;
            height: 48px;
            outline: none;
            border: none;
            text-align: center;
            line-height: 30px;
            border-radius: 5px;
            box-shadow: rgb(0 0 0 / 20%) 0px 2px 6px 0px;
            transition: background-color 0.3s ease-out 0s;
            font-size: 1rem;
        }
        
        button.revise {
            border-radius: 15px;
            margin-left: 42%;
            position: absolute;
            margin-top: -55px;
            left: 15%;
        }
        
        button.close {
            border-radius: 15px;
            margin-left: 64%;
            position: absolute;
            margin-top: -55px;
            left: 15%;
        }
        