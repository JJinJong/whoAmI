@charset "UTF-8";

*{
   box-sizing: border-box;
    font-family: 'Spoqa Han Sans Neo', 'sans-serif' !important;
    letter-spacing: -0.7px;
}

/* 알림창  */
.fVNCIX {
            position: fixed;
            top: 0px;
            width: 350px;
            height: 100vh;
            background-color: rgb(255, 255, 255);
            box-shadow: rgb(0 0 0 / 30%) 5px 0px 10px;
            z-index: 1000;
            border-right: 5px solid #4b4d91;
            overflow: auto;
            padding-bottom: 20px;
            opacity: 0;
            left: -350px;
            transition-property: left, opacity;
            transition-duration: 0.3s;
      	}
		.logo {
		    position: relative;
    		left: -15px;
    		top: -2px;
		}        
		.header_top_infowrapper {
			position: relative;
    		margin-left: -15px;
		}

        :root {
            --swiper-navigation-size: 44px;
            /* --swiper-navigation-color: var(--swiper-theme-color); */
        }
        
        :root {
            --swiper-theme-color: #007aff;
        }

        .fVNCIX .notifications__header {
            padding: 15px;
            display: flex;
            -webkit-box-pack: justify;
            justify-content: space-between;
            box-shadow: rgb(0 0 0 / 20%) 0px 3px 6px;
            -webkit-box-align: center;
            align-items: center;
            position: fixed;
            top: 0px;
            width: 345px;
            background-color: rgb(255, 255, 255);
        }
        
        .fVNCIX .notifications__header__title {
            display: flex;
            -webkit-box-align: center;
            align-items: center;
        }
        
        .fVNCIX .notifications__header__close {
            display: flex;
            -webkit-box-align: center;
            align-items: center;
            cursor: pointer;
        }
        
        .fVNCIX .notifications__main {
            margin-top: 65px;
            padding: 0px 15px 20px;
            overflow-x: hidden;
        }
        
        .jTwwkK {
            padding: 15px;
            box-shadow: rgb(0 0 0 / 20%) 0px 2px 4px;
            border-radius: 5px;
            font-size: 0.9rem;
            display: flex;
            width: 100%;
            cursor: pointer;
        }
        
        .jTwwkK + .notification {
            margin-top: 15px;
        }
        
        .jTwwkK .notification__icon {
            width: 30px;
            display: flex;
            -webkit-box-pack: center;
            justify-content: center;
            -webkit-box-align: center;
            align-items: center;
        }
        
        .jTwwkK .notification__main {
            -webkit-box-flex: 1;
            flex-grow: 1;
            margin-left: 10px;
            width: calc(100% - 40px);
        }
        
        .jTwwkK .notification__icon img {
            width: 70%;
        }
        
        img {
            border: 0px;
            height: auto;
        }
        
        .jTwwkK .notification__main__time {
            font-size: 0.8rem;
            text-align: right;
        }
        
        .jTwwkK .notification__main__contents {
            margin-top: 5px;
        }
        
        .fVNCIX .notifications__footer {
            padding: 0px 15px;
        }
        
        






/* notice 공지사항 부분 */

.notice_wrapper{
	overflow: auto;
    position: fixed;
    top: 0px;
    width: 350px;
    height: 100vh;
    background-color: rgb(255, 255, 255);
    box-shadow: rgb(0 0 0 / 30%) -5px 0px 10px;
    z-index: 5000;
    border-left: 5px solid #4b4d91;
    display: flex;
    flex-direction: column;
    opacity: 0;
    right: -350px;
    transition: all 0.3s ease-in 0s;
}

/* notice header */

.notice_header{
	position: fixed;
    top: 0px;
    width: 345px;
    padding: 15px;
    justify-content: space-between;
    box-shadow: rgb(0 0 0 / 20%) 0px 3px 6px;
    align-items: center;
    background-color: rgb(255, 255, 255);
    display: flex;
}

.notice_header_title{
	display: flex;
    align-items: center;
}

.notices_header_title svg{
	display: flex;
    align-items: center;
} 

.notice_header_colse svg{
	width: 20px;
    height: 20px;
}

/* notice main */
.notice_main{
	margin-top: 65px;
    padding: 0px 15px 20px;
    flex-grow: 1;
}

.notice_main_text{
	padding: 15px;
    box-shadow: rgb(0 0 0 / 20%) 0px 2px 4px;
    border-radius: 5px;
    font-size: 0.9rem;
    cursor: pointer;
}

.notice_main_day{
	display: flex;
    justify-content: space-between;
}

.notice_main_day_badge{
	background-color: #4b4d91;
    color: rgb(255, 255, 255);
    padding: 0px 5px 2px;
    font-size: 10px;
    border-radius: 10px;
}

.notice_main_regist_time{
    flex-grow: 1;
    font-size: 0.8rem;
    text-align: right;

}

.notice_main_content{
	margin-top: 5px;
}


/* 
헤더  
*/
header.header_wrapper{
    box-shadow: rgb(240 240 240) 0px -1px 0px 0px inset;
    padding: 0px 16px;
    background-color: rgb(255, 255, 255);
    position: fixed;
    top: 0px;
    width: 100%;
	z-index: 400;
	    left: 0;
}

/* 헤더 첫번째 상단 */
div.header_top{
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    margin: 0px auto;
    max-width: 1164px;
}

div.header_top_infowrapper{
    display: flex;
}

div.header_top_infowrapper_link{
    font-size: 12px;
    color: rgb(134, 134, 134);
    margin: 10px 10px 8px;
    letter-spacing: -0.65px;
    text-align: center;
    cursor: pointer;
}

/* 로그인 버튼 */
div.header_top_infowrapper button.button_login{
    color: #4b4d91;
    cursor: pointer;
    border: none;
    outline: none;
    user-select: none;
    background-color: rgba(0, 0, 0, 0);
    padding: 0;
    height: 40px;
}

/* 회원가입버튼 */
div.header_top_infowrapper button.button_join{
    margin-left: 24px;
    color: rgb(72, 72, 72);
    cursor: pointer;
    border: none;
    outline: none;
    user-select: none;
    background-color: rgba(0, 0, 0, 0);
    padding: 0;
}

/* 줄 */
div.header_line{
    height: 1px;
    width: 100vw;
    box-shadow: 
        rgb(240 240 240) 0px -1px 0px 0px inset, 
        rgb(240 240 240) 0px 1px 0px 0px inset;
}

/* 하단 헤더 (로고 및 메뉴버튼) */
div.header_header{
    position: relative;
    display: flex;
    align-items: center;
    height: 58px;
    max-width: 1164px;
    margin: 0px auto;
}

div.header_nav_wrapper{
    position: relative;
    flex-grow: 1;
    display: flex;
    align-items: center;
}

div.logo_img{
    border: 0px;
    height: auto;
    cursor: pointer;
    display: flex;
    align-items: center;
    margin-right: 10px;
}

div.logo_img img{
    width: 200px;
    height: auto;
}

div.header_nav{
    display: flex;
    margin-left: 80px;
}

div.header_nav_button_wrapper{
    font-size: 18px;
    font-weight: 500;
}

button.header_nav_button{
    font-size: 18px;
    color: rgb(72, 72, 72);
    font-weight: normal;
    white-space: pre;
    border-radius: 1px;
    
    cursor: pointer;
    border: none;
    outline: none;
    background-color: rgba(0, 0, 0, 0);
    padding: 0;
    user-select: none;
    width: 120px;
}

/* 알림 마이페이지 글쓰기 */
div.header_nav_icon_wrapper{
    flex-shrink: 0;
    display: flex;
    position: relative;
    align-items: center;
}

div.header_nav_icon{
    margin-left: 25px;
    display: block;
    align-items: center;
    position: relative;
}

div.header_nav_icon img{
    width: 23px;
    height: 23px;
    margin-top: 10px;
    cursor: pointer;
    padding: 2px;
    
}

button.header_nav_icon_button{
    background-color: #4b4d91;
    padding: 6px 20px;
    cursor: pointer;
    font-weight: bold;
    color: rgb(255, 255, 255);
    font-size: 15px;
    border-radius: 6px;
    border: none;
    outline: none;
}


