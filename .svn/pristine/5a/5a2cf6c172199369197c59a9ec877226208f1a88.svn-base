/* 
메인 footer
메인 푸터
*/
a{
	text-decoration-line: none;
}

footer.footer_wrapper{
    position: relative;
    margin-top: 56px;
    padding: 40px 0px 48px;
    box-shadow: rgb(220 220 220) 0px 1px 0px 0px inset;
    background-color: rgba(240, 240, 240, 0.38);
    margin-bottom: 0px;
    overflow: auto;
}

.footer_wrapper_footer{
	position: relative;
    margin-top: 56px;
    padding: 40px 0px 48px;
    box-shadow: rgb(220 220 220) 0px 1px 0px 0px inset;
    background-color: rgba(240, 240, 240, 0.38);
    margin-bottom: 0px;
    overflow: auto;
}

.footer_copyright_wrapper_wrapper{
	    display: flex;
    margin: 0px auto;
    width: 100%;
    justify-content: center;
	
}

section.footer_copyright_wrapper{
    display: flex;
    margin: 0px auto;
    width: 100%;
    justify-content: center;
}

div.footer_info{
    display: flex;
    flex-direction: column;
    margin-left: -120px;
    width: 660px;
}

div.footer_nav{
    display: flex;
    width: 700px;
    -webkit-box-align: center;
    align-items: center;
}

div.footer_nav a.footer_nav_clause{
    cursor: pointer;
    font-size: 12px;
    font-weight: 500;
    letter-spacing: -0.6px;
    color: rgb(72, 72, 72);
}

div.footer_columbar{
    padding-left: 6px;
    padding-right: 6px;
    color: rgb(187, 187, 187);
}

a.footer_nav_priateInfo{
    cursor: pointer;
    font-size: 12px;
    font-weight: bold;
    letter-spacing: -0.6px;
    color: rgb(72, 72, 72);
}

a.footer_nav_alarm{
    cursor: pointer;
    font-size: 12px;
    font-weight: 500;
    letter-spacing: -0.6px;
    color: rgb(72, 72, 72);
}

div.footer_companyInfo{
    display: flex;
    align-items: flex-start;
    margin-top: 12px;
}

div.footer_companyInfo_content_wrapper{
    flex-grow: 1;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}

span.footer_companyInfo_content{
    width: fit-content;
    font-size: 11px;
    color: rgb(134, 134, 134);
}

span.footer_companyInfo_content span{
    color: rgb(72, 72, 72);
    margin-left: 8px;
}


/* 어플 부분  */
article{
    margin-left: 75px;
}

div.footer_app{
    display: flex;
    align-items: center;
}

div.footer_app_img{
    display: flex;
    width: 200px;
    align-items: center;
}

div.footer_app_img img{
    width: 40px;
    height: 40px;
    border-radius: 8px;
}

div.footer_app_content{
    margin-left: 8px;
}

div.footer_app_content_title{
    font-weight: 400;
    width: 130px;
}

div.footer_app_content_detail{
    font-size: 12px;
    color: rgb(134, 134, 134);
}

div.footer_app_link{
    flex-grow: 1;
    display: flex;
    flex-wrap: wrap;
    width: max-content;
}

div.footer_app_link a{
    display: flex;
}

div.footer_app_link img{
    margin-left: 10px;
    width: 100px;
    height: 30px;
    border: 0px;
}

/* 뒤로가기 이미지 */
div.payment_arrow{
	position: relative;
    cursor: pointer;
    background-color: #4b4d91;
    color: white;
    width: 35px;
    height: 35px;
    border-radius: 50%;
    font-size: 1.5rem;
    box-shadow: rgb(0 0 0 / 30%) 0px 2px 3px 0px;
    margin-right: 10px;
}

div.payment_arrow svg{
	position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

div.pament_title{
	display: flex;
    align-items: center;
    font-size: 20px;
}

div.payment_content_wrapper{
	border-top: 13px solid rgb(255 0 0 / 28%);
	box-shadow: 0px 3px 6px 0 rgb(0 0 0 / 20%);
    border-radius: 5px;
    padding: 20px;
    background-color: #fff;
}

.payment_content_header{
	margin-top: 30px;
    border-bottom: 1px solid rgb(221, 221, 221);
    padding-bottom: 20px;
}

.payment_content_header_icon{
	    text-align: center;
}

.payment_content_header_icon img{
	border: 0px;
    height: auto;
}

.payment_content_header_text{
	text-align: center;
    margin-top: 10px;
    line-height: 1.6rem;
    font-size: 1.1rem;
}	

div.footer_app{
    width: 500px;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    flex-direction: row;
    position: relative;
}       

.expertJoin{
	position: absolute;
    position: absolute;
    right: 89px;
    top: 90px;
    cursor: pointer;
}

.footer_btn{
	background-color: rgb(140, 204, 201);
    color: rgb(255, 255, 255);
    padding: 6px 20px;
    border-radius: 3px;
    font-weight: bold;
    font-size: 15px;
    border: none;
    outline: none;
    width: 200px;
    height: 50px;
}

.modal_back{
	position: absolute;
    width: 100%;
    height: 243px;
    z-index: 100;
    text-align: center;
    display: none;
}

.modal_content{
	width: 98%;
    height: 96%;
    background: #fff;
    border-radius: 10px;
    text-align: center;
    box-sizing: border-box;
    line-height: 23px;
    cursor: pointer;
    box-shadow:rgb(0 0 0 / 10%) 0px 8px 16px 0px;
    display: inline-block;
    margin-top: 4px; 
}

.modal_content span{
    margin-left: 10px;
    float: left;
    display: inline-block;
    margin-top: 10px;
    color: #8cccc9;
}

.modal{
	display: inline-block;
    margin: -157px auto;
    border-radius: 10px;
    width: 24%;
    height: 81%;
    background-color: rgb(140 204 201);
    z-index: 100;
    text-align: center;
}

input[type=text]{
    appearance: none;
    background: #ffffff;
    border-radius: 0.375em;
    border: none;
    border: solid 1px rgb(109 119 124 / 84%);
    color: inherit;
    display: inline-block;
    outline: 0;
    padding: 0 1em;
    text-decoration: none;
    width: 80%;
    height: 2.75em;
}

.code_btn{
    background-color: rgb(140, 204, 201);
    color: rgb(255, 255, 255);
    padding: 6px 20px;
    border-radius: 3px;
    font-weight: bold;
    font-size: 15px;
    border: none;
    outline: none;
    width: 67px;
    height: 34px;
    cursor: pointer;
}

@media (max-width:480px){
	div.header_nav_wrapper{
	position: relative;
    width: 263px;
    flex-grow: 1;
    display: flex;
    align-items: flex-start;
    flex-direction: column;
	}
	
	div.header_header{
		height: 87px;
		align-items: flex-start;
	}
	
	div.wrapper_top {
	    position: relative;
	    margin: 0 auto;
	    display: flex;
	    margin-top: 129px;
	    height: 48px;
	    max-width: calc(1164px + 32px);
	    bottom: -14px;		
	}
	
	div.header_nav{
		display: flex;
    	margin-left: 0;
	}
	
	div.header_nav_icon_wrapper{
		margin-bottom: 10px;
	}
	
}

