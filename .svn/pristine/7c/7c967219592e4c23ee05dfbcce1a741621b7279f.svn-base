        .cbIfjw .on-off.off {
            background-color: rgb(255, 255, 255);
        }

        .cbIfjw .on-off {
            flex-shrink: 0;
            cursor: pointer;
            position: relative;
            width: 40px;
            height: 20px;
            border-radius: 15px;
            box-shadow: rgb(0 0 0 / 40%) 1px 2px 3px 0px inset;
            background-color: rgb(246, 239, 207);
            opacity: 0.95;
            transition: background-color 0.2s ease 0s;
        }

        .cbIfjw .on-off.off .switch {
            left: 0px;
        }

        .cbIfjw .on-off .switch {
            background-color: white;
            position: absolute;
            top: 50%;
            left: 18px;
            width: 22px;
            height: 22px;
            transform: translateY(-50%);
            box-shadow: rgb(0 0 0 / 50%) 0px 2px 4px 0px;
            border-radius: 50%;
            transition: left 0.2s ease 0s;
        }

        .cbIfjw .label.off {
            color: rgb(170, 170, 170);
        }

        .cbIfjw .label {
            margin-left: 5px;
            font-size: 0.9rem;
            color: rgb(68, 68, 68);
            transition: color 0.2s ease 0s;
        }

