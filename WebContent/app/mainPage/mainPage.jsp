<script>
	const navigators = document.querySelectorAll("div.navigators__navigator");
	const banners = document.querySelector("div.banners");
	console.log(navigators);
	var count = 0;
	navigators.forEach(function(value, index, ar) {
		ar[index].addEventListener("click", function(event) {
			count = index;
			banners.style.transform = "translate(-" + count * 100 + "vw)";
			$(navigators).removeClass("navigators__navigator--selected");
			$(this).addClass("navigators__navigator--selected");
		})
	});
	var time = setInterval(function() {
		count++;
		count = count == 8 ? 0 : count;
		banners.style.transform = "translate(-" + count * 100 + "vw)";
	}, 3000);
</script>
